
<div id="sign-in-form">
  <h1><%= title %></h1>
<form action="/users/create-session" method="POST">
  <input type="email" name="email" placeholder="Email" class="email" required />
  <input type="password" name="password" placeholder="Password" class="password" required />
  <input type="submit" value="Sign In" class="sign-in"/>
</form>
<p>
  <a href="/users/auth/google">Google Sign In / Sign Up</a>
</p>
</div>